<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.asu.seatr.opetest.models">
    <class name="student_response" table="student_response">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property name="user_id"/>
        <property name="correct"/>
        <property name="class_question_id"/>
        <property name="duration"/>
        <property name="explanation_duration"/>
        <property name="date"/>
        <property name="question_type"/>
        <property name="response_type"/>
        <property name="source"/>
        <property name="data"/>
         
    </class>
</hibernate-mapping>